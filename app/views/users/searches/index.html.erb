<% if @searched_users.present? %>
  <% @searched_users.each do |u| %>
    <%= u.email %>
    <%= link_to 'リクエスト', new_user_request_path(u) %>
  <% end %>
<% end %>
<%= form_with url: users_searches_path, method: :get, local: true do |f| %>
  <%= f.label :email, '名前' %>
  <%= f.text_field :email %>
  <%= f.submit '検索' %>
<% end %>